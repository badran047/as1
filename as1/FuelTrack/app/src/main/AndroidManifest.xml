<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="badran.fueltrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Badran-FuelTrack"
        android:supportsRtl="true"
        android:name="badran.fueltrack.FuelTrackApp"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayEntryActivity" android:label="DisplayEntryActivity"/>
        <activity android:name=".EditEntryActivity" android:label="EditEntryActivity"/>
    </application>

</manifest>
